{
  "name": "Hugo-Doks",
  "version": "0.0.0",
  "description": "Doks theme",
  "author": "Hyas",
  "license": "MIT",
  "scripts": {
    "create": "hugo new content",
    "dev": "hugo server -D -E -F -M -N -O --disableFastRender --noHTTPCache --bind=0.0.0.0 --baseURL='http://localhost:1313/' --logLevel=info",
    "format": "prettier **/** -w -c",
    "build": "hugo --minify --gc --logLevel info",
    "preview": "vite preview --outDir public"
  },
  "dependencies": {
    "@hyas/doks-core": "^1.6.1",
    "@hyas/images": "^3.2.0",
    "@hyas/inline-svg": "^1.1.0",
    "@hyas/seo": "^2.3.0",
    "@tabler/icons": "^3.33.0",
    "gethyas": "^2.4.2"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "vite": "^7.0.5"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
